{
  "name": "keybinding-conflict-scanner",
  "displayName": "Keybinding Conflict Scanner",
  "description": "Automatically detect and resolve keybinding conflicts between VS Code extensions",
  "version": "0.1.1",
  "publisher": "rhslvkf",
  "author": {
    "name": "FullMoon"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhslvkf/keybinding-conflict-scanner.git"
  },
  "bugs": {
    "url": "https://github.com/rhslvkf/keybinding-conflict-scanner/issues"
  },
  "homepage": "https://github.com/rhslvkf/keybinding-conflict-scanner#readme",
  "keywords": [
    "keybinding",
    "shortcut",
    "conflict",
    "scanner",
    "keyboard",
    "extensions",
    "productivity"
  ],
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keybinding-conflict-scanner.scan",
        "title": "Keybinding Conflict Scanner: Scan Conflicts"
      }
    ],
    "configuration": {
      "title": "Keybinding Conflict Scanner",
      "properties": {
        "keybindingConflictScanner.autoDetect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically detect conflicts when extensions are installed"
        },
        "keybindingConflictScanner.showNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Show notifications when conflicts are detected"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/vsce": "^2.19.0"
  }
}
